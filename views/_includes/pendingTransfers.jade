#pendingTransfers.modal.fade(tabindex="-1", role="dialog", aria-labelledby="pendingTransfersLabel", aria-hidden="true")
    .modal-dialog
        .modal-content
            .modal-header
                button.close(type="button", data-dismiss="modal")
                    span(aria-hidden="true") &times;
                    span.sr-only Close
                h4.modal-title#pendingTransfersLabel Pending Transfers
            .modal-body
                .row
                    .col-md-6
                        h5.text-center <i class="fa fa-dropbox"></i> <i class="fa fa-long-arrow-right"></i> <i class="ico-moon-google-drive"></i>
                        ul#transfersToGoogle.nav.nav-pills.nav-stacked.transfer-list
                            li.disabled
                                a Empty
                        p.text-center
                            button.btn.btn-success#startTransferToGoogle(type="button", data-loading-text="transfering...") Start transfer
                    .col-md-6
                        h5.text-center <i class="ico-moon-google-drive"></i> <i class="fa fa-long-arrow-right"></i> <i class="fa fa-dropbox"></i>
                        ul#transfersToDropbox.nav.nav-pills.nav-stacked.transfer-list
                            li.disabled
                                a Empty
                        p.text-center
                            button.btn.btn-success#startTransferToDropbox(type="button", data-loading-text="transfering...") Start transfer