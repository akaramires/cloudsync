.todo.file-list
    - if(typeof list !== "undefined")
        .todo-search
            | /
        ul
            - if (list.length > 0)
                - each item in list
                    li(class=(item.mimeType === 'application/vnd.google-apps.folder')? "folder" : "file")
                        - if (item.mimeType === 'application/vnd.google-apps.folder')
                            .todo-icon.fa.fa-folder-open-o
                        - else
                            .todo-icon.fa.fa-file-o
                        .todo-content
                            h4.todo-name
                                = ' ' + item.title
                            input.json-data(type='hidden', value=JSON.stringify(item))
                            input.checked.hide(type='checkbox')
            - else
                li.folder
                    .todo-icon.fa.fa-file-o
                    .todo-content
                        h4.todo-name Not found

